{
  "name": "vml",
  "version": "1.0.0",
  "description": "Vande Mataram Library",
  "scripts": {
    "start": "per-env",
    "start:development": "npm run buildServer && npm run serve",
    "start:production": "npm run buildServer && npm run buildClient && npm run serve",
    "buildServer": "tsc",
    "buildClient": "webpack --config config/webpack.config.prod.js",
    "serve": "per-env",
    "serve:development": "node build/server/devServer",
    "serve:production": "nohup node -r dotenv/config build/server/prodServer &",
    "lint": "tslint -c ./tslint.json  './src/**/*.ts' './src/**/*.tsx'"
  },
  "pre-commit": [
    "lint",
    "buildServer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vandemataramlib/vml-web"
  },
  "keywords": [
    "sanskrit"
  ],
  "author": "prashaantt@gmail.com",
  "license": "ISC",
  "dependencies": {
    "auth0-lock": "10.2.1",
    "boom": "3.2.2",
    "core-js": "2.4.1",
    "fbjs": "0.8.4",
    "handlebars": "4.0.5",
    "hapi": "13.5.0",
    "history": "3.0.0",
    "inert": "4.0.1",
    "isomorphic-fetch": "2.2.1",
    "jsonapi-serializer": "3.3.0",
    "lodash": "4.15.0",
    "material-ui": "0.15.4",
    "mobx": "2.4.4",
    "mobx-react": "3.5.5",
    "react": "15.3.1",
    "react-anything-sortable": "1.6.1",
    "react-dom": "15.3.1",
    "react-router": "2.7.0",
    "react-router-scroll": "0.3.2",
    "react-tap-event-plugin": "1.0.0",
    "sanscript": "0.0.2",
    "vision": "4.1.0",
    "vml-common": "https://github.com/vandemataramlib/vml-common.git"
  },
  "devDependencies": {
    "awesome-typescript-loader": "2.1.1",
    "dotenv": "2.0.0",
    "exiting": "2.0.1",
    "hapi-webpack-dev-middleware": "1.0.1",
    "hapi-webpack-hot-middleware": "1.0.2",
    "mobx-react-devtools": "4.2.5",
    "per-env": "1.0.2",
    "pre-commit": "1.1.3",
    "react-hot-loader": "3.0.0-beta.2",
    "tslint": "3.15.1",
    "typescript": "1.8.10",
    "webpack": "1.13.2",
    "webpack-hot-middleware": "2.12.2",
    "webpack-notifier": "1.4.1"
  }
}
